@model IEnumerable<shoopsupermarket.Models.Proveedor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PHONE1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PHONE2)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PHONE1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PHONE2)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PROV_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PROV_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PROV_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
